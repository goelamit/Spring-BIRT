            <method name="open"><![CDATA[importPackage(Packages.org.springframework.context);
importPackage(Packages.org.springframework.web.context.support );

//ServletContext
var sc = reportContext.getHttpServletRequest().getSession().getServletContext();
//ApplicationContext 
var spring = WebApplicationContextUtils.getWebApplicationContext(sc);

count = 0;
service = spring.getBean("reportService");
customerDataSetList=service.getAllCustomers();
]]></method>
            <method name="fetch"><![CDATA[if(customerDataSetList!=null &amp;&amp;  customerDataSetList.size()>0 &amp;&amp; count < customerDataSetList.size()){


       row["customerNumber"] = customerDataSetList.get(count).getCustomerNumber();
       row["customerName"] = customerDataSetList.get(count).getCustomerName();
       row["contactLastName"] = customerDataSetList.get(count).getContactLastName();
       row["contactFirstName"] = customerDataSetList.get(count).getContactFirstName();
       row["phone"] = customerDataSetList.get(count).getPhone();
       row["addressLine1"] = customerDataSetList.get(count).getAddressLine1();
       row["addressLine2"] = customerDataSetList.get(count).getAddressLine2();
       row["city"] = customerDataSetList.get(count).getCity();
       row["state"] = customerDataSetList.get(count).getState();
       row["postalCode"] = customerDataSetList.get(count).getPostalCode();
       row["country"] = customerDataSetList.get(count).getCountry();
       row["salesRepEmployeeNumber"] = customerDataSetList.get(count).getSalesRepEmployeeNumber();
       row["creditLimit"] = customerDataSetList.get(count).getCreditLimit();      
       count++;
       return true;
}

return false; 




]]></method>
